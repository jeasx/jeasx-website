{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "devCommand": "npm run dev",
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/vercel.js"
    }
  ],
  "functions": {
    "api/vercel.js": {
      "includeFiles": "./**/*"
    }
  },
  "outputDirectory": "build"
}
